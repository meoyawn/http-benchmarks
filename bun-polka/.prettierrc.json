{
  "arrowParens": "avoid",
  "proseWrap": "always",
  "semi": false,
  "trailingComma": "all",
  "plugins": [
    "prettier-plugin-packagejson",
    "prettier-plugin-organize-imports"
  ],
  "overrides": [
    {
      "files": ["*.yml", "*.yaml"],
      "options": {
        "proseWrap": "preserve"
      }
    }
  ]
}
